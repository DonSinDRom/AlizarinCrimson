.online__include
  display flex
  flex 0 0 15%
  height calc(100% - 90px)
  overflow-x hidden
  color #000

.online__md-content
  width 100%

.online__here
.online__counter
  color #000
  line-height 1
  text-align center
  margin 5px 0
  font-size 14px

.user__2profile
  display inline
  width 16px
  min-width 16px
  height 16px
  margin-left 6px
  text-transform none
  border-radius 50% !important
  background-color #eba400
  color #fff
  font-size 9px
  & .md-button-inner
    padding 2px 6px

.user__2chat
  display inline
  font-size 12px
  color #aaa !important
  text-transform capitalize
  white-space nowrap
  overflow hidden
  text-overflow ellipsis
  & .md-button-inner
    padding 2px 6px
  &:hover
    background-color transparent !important
    text-decoration underline
