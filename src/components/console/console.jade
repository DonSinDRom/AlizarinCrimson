md-toolbar.console__include(ng-controller="ConsoleCtrl")

  md-slider(ng-model="volume" ng-change="changeVolume(volume)" min="0" max="100" step="1" aria-label="volume-slider")
  img.console__logo(ng-src="assets/images/logo.png" ng-click="data()")
  md-button.console__login-btn(ng-show="!load.signed" ng-click="showLoginModal($event)" aria-label="console-login") {{ 'consoleLogIn' | translate }}
  p.console__stat
    | {{ 'consoleListeners' | translate }}:&nbsp;
    span.console__listeners {{ users.listeners }}
    br
    | {{ 'consoleActiveUsers' | translate }}:&nbsp;
    span.console__active-users {{ users.active }}
  md-button.console__buttton--upload(ng-click="openUploadBar($event)" aria-label="uploader-btn")
    ng-md-icon(icon="file_upload" size="24")
  p.console__user-limit(ng-show="load.signed") {{ 'consoleYouCanUpload' | translate }}&nbsp;
    time.console__limit {{ user.uplim | readableTime }}
  p.console__links
    //a.console__link {{ 'consoleDonateMe' | translate }}
    a.console__link(ng-click="changeTheme()") {{ 'consoleChangeTheme' | translate }}
    //a.console__link) {{ 'consoleRTFM' | translate }}
  md-button.console__login-btn(ng-show="load.signed" ng-click="logout()" aria-label="console-logout") {{ 'consoleLogOut' | translate }}

  blockquote.muzoblob__blockquote
    p.console__greating(greeting="")

