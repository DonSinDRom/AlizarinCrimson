.md-sidenav-left
  & .console__switcher-cont
    background-color darkPrimaryColor
.console__include
  background-color darkPrimaryColor !important

@media screen and (min-width: 960px)
  .md-sidenav-left
    & .console__switcher-cont
      background-color darkPrimaryColor


.console
  & md-slider
    & .md-track
      background-color #777
    & .md-track-fill
      background-color #fff
    & .md-thumb
      &:after
        border-color #fff
        background-color #fff
    & .md-focus-thumb
      background-color #fff
    & .md-focus-ring
      background-color rgba(255,255,255,.2)
.console__buttton--upload
  background-color accentColor
  color #fff
  & svg
    fill #fff
.console__username
  color accentColor
.console__link
  color accentColor
.switcher
  height 100%
  margin 5px
  &.play
    & .c
      fill accentColor
    & .b3
    & .b2
    & .b1
      stroke accentColor
